{
    "name": "express-es6-zachyu-server",
    "version": "0.1.0",
    "description": "Seed project for easily getting started with express for es6",
    "scripts": {
        "clean": "rm -rf dist",
        "start": "node ./server.js --exec babel-node",
        "dev": "concurrently \"npm run start\" \"npm run client\"",
        "client": "cd client && yarn start",
        "serve": "yarn build-server && node dist/server.js",
        "test": "nodemon --exec 'mocha --compilers js:babel-core/register -R min' ",
        "lint": "eslint .",
        "heroku-postbuild": "cd client && yarn && yarn run build",
        "build-server": "yarn clean && babel . -d dist --presets es2015,stage-2 --ignore spec.js,node_modules,client,dist",
        "build-client": "cd client && yarn && yarn build",
        "build": "yarn && yarn build-server && yarn build-client "
    },
    "backup_scritps": {
        "build": "babel . -d dist --presets es2015,stage-2 --ignore spec.js,node_modules"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tomyitav/express-es6-starter.git"
    },
    "keywords": [ "Node.js", "Javascript", "Express" ],
    "author": "Zach Yu <zachyu.tw@gmail.com>",
    "dependencies": {
        "@google/maps": "^0.5.5",
        "axios": "^0.18.0",
        "bcrypt-nodejs": "^0.0.3",
        "body-parser": "^1.18.3",
        "chai": "^4.1.2",
        "child_process": "^1.0.2",
        "compression": "^1.7.3",
        "concurrently": "^4.0.1",
        "connect-flash": "^0.1.1",
        "connect-gzip-static": "^2.1.1",
        "connect-multiparty": "^2.2.0",
        "cookie-parser": "^1.4.3",
        "cookie-session": "^2.0.0-beta.3",
        "cors": "^2.8.0",
        "dotenv": "^6.1.0",
        "email-templates": "^5.0.2",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "express": "4.14.0",
        "express-session": "^1.15.6",
        "express-static-gzip": "^1.1.3",
        "faker": "^4.1.0",
        "form-data": "^2.3.3",
        "formidable": "^1.2.1",
        "helmet": "^3.13.0",
        "jsonwebtoken": "^8.3.0",
        "lodash": "^4.15.0",
        "mockjs": "^1.0.1-beta3",
        "moment": "^2.22.2",
        "mongoose": "^4.8.6",
        "morgan": "^1.8.2",
        "multer": "^1.4.1",
        "needle": "^2.2.4",
        "nodemailer": "^5.0.0",
        "object-to-formdata": "^1.5.2",
        "passport": "^0.4.0",
        "passport-custom": "^1.0.5",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "passport-openid": "^0.4.0",
        "prop-types": "^15.6.2",
        "pug": "^2.0.3",
        "socket.io": "^2.1.1",
        "winston": "^2.3.1",
        "winston-daily-rotate-file": "^1.4.6",
        "ws": "^6.0.0"
    },
    "devDependencies": {
        "babel-cli": "6.16.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "9.0.0",
        "babel-jest": "^23.6.0",
        "babel-plugin-inline-import": "^3.0.0",
        "babel-plugin-transform-async-to-module-method": "^6.24.1",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.16.0",
        "babel-preset-es2015": "^6.16.0",
        "babel-preset-react": "^6.5.0",
        "babel-preset-stage-0": "^6.16.0",
        "eslint": "5.6.0",
        "eslint-config-airbnb": "^17.0.0",
        "eslint-plugin-import": "^2.0.1",
        "eslint-plugin-mocha": "^5.1.0",
        "eslint-plugin-react": "^7.10.0",
        "jest": "^23.6.0",
        "nodemon": "^1.9.1",
        "regenerator-runtime": "^0.12.1"
    },
    "eslintConfig": {
        "parser": "babel-eslint",
        "extends": [ "airbnb/base", "plugin:import/errors" ],
        "rules": {
            "no-use-before-define": 0,
            "arrow-body-style": 0,
            "dot-notation": 0,
            "no-console": 0
        },
        "env": {
            "mocha": true
        },
        "jest": {
            "transform": {}
        }
    }
}
